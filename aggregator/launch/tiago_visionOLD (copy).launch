<launch>
  <!-- Openni2 launch to use external Orbbec camera -->
  <include file="$(find openni2_launch)/launch/openni2.launch"/>
  <!-- Cagbal code to detect object and people -->
  <include file="$(find cob_people_object_detection_tensorflow)/launch/alltogether.launch"/>
  <!-- RVIZ visualisation node -->
  <node pkg="rviz" type="rviz" name="$(anon rviz)" args="/home/hearts/.rviz/default.rviz" />
  <!-- Variable to input on the teminal window -->
  <arg name = "bag_name" />
  <!-- Rosbag recording -->
  <node pkg ="rosbag" type="record" name="rosbag" args="record -o /object_detection/detections" />

<!-- $(arg bag_name).bag -->
  <!-- Aggregator node -->
  <!-- <node name="aggregator" pkg="aggregator" type="decision_making_cb.py" respawn="true" > -->
  <!-- </node> -->

</launch>
